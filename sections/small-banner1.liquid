


{%- style -%}
.section-{{ section.id }}-padding {
  padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
  padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
}




  

@media screen and (min-width: 750px) {
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top }}px;
    padding-bottom: {{ section.settings.padding_bottom }}px;
  }
}
{%- endstyle -%} 

<div class="section-{{ section.id }}-padding">
    
    <!-- section-9 start -->
    <div class="section-9">
        <div class="wrapper">
          <div class="pure-g">
            <div class="pure-u-1 pure-u-md-24-24">
              <div class="sec9-content">
                <h3 class="h3"> {{section.settings.heading3}} </h3>
                <p>
                    {{section.settings.paragraph}}
                </p>
                <h5 class="h5"> {{section.settings.heading5}} </h5>
                <button>Conoce los requisitos</button>
              </div>
            </div>
          </div>
        </div>
      </div>



  
</div>

{% schema %}
{
"name": "small-banner1",
"settings": [
  {
    "type": "text",
    "label": "Title",
    "id": "heading3"
  },
  {
    "type": "text",
    "label": "Title",
    "id": "paragraph"
  },
  

  {
    "type": "text",
    "label": "Title",
    "id": "heading5"
  },

  {
    "type": "range",
    "id": "padding_top",
    "min": 0,
    "max": 100,
    "step": 4,
    "unit": "px",
    "label": "t:sections.all.padding.padding_top",
    "default": 36
  },
  {
    "type": "range",
    "id": "padding_bottom",
    "min": 0,
    "max": 100,
    "step": 4,
    "unit": "px",
    "label": "t:sections.all.padding.padding_bottom",
    "default": 36
  }
],
"presets": [
  {
    "name": "small-banner1"
  }
]
}


{% endschema %}
