    padding: 80px;
} -->




@media screen and (min-width: 750px) {
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top }}px;
    padding-bottom: {{ section.settings.padding_bottom }}px;
  }
}
{%- endstyle -%}

<div class="section-{{ section.id }}-padding">
    
    <div class="tahir-section-3">
        <div class="container">
          <div class="pure-g">
            <div class="pure-u-1">
              <h3 class="sec-3-h2 H3"><span> {{section.settings.h3_hed_1}} </span> {{section.settings.h3_hed_2}}</h3>
            </div>
          </div>
     
          <div class="pure-g">
            <div class="pure-u-1 pure-u-md-12-24">
              <ul class="sec-3-li body-1">
                <li>{{section.settings.sec3_li_1}}</li>
                <li>{{section.settings.sec3_li_2}}</li>
                <li>{{section.settings.sec3_li_3}}</li>
              </ul>
            </div>
            <div class="pure-u-1 pure-u-md-12-24">
              <ul class="sec-3-li sec-3-li-2 body-1">
                <li>{{section.settings.sec3_li_4}}
                </li>
                <li>{{section.settings.sec3_li_5}}
                </li>
                <li>{{section.settings.sec3_li_6}}
                </li>
                <li>{{section.settings.sec3_li_7}}</li>
                <li>{{section.settings.sec3_li_8}}</li>
              </ul>
            </div>
          </div>   
    
        </div>
      </div>


  

</div>

